    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "jwk-to-pem": "^2.0.5",
    "moment": "^2.30.1",
    "mssql": "^10.0.2",
    "multer": "^1.4.5-lts.1",
