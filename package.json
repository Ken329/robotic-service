{
  "name": "robotic-service",
  "version": "1.0.0",
  "description": "The robotic service",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "start:dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start:debug": "npx ts-node-dev --respawn --transpile-only --debug src/index.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ken329/robotic-service.git"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.25",
    "@types/node-rsa": "^1.1.4",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "express": "^4.18.3",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.0"
  },
  "author": "Ken Liau",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.4.5",
    "node-rsa": "^1.1.1"
  }
}
